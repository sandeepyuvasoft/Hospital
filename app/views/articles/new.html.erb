<h3><%=link_to "Home" ,:root%></h3>
<%= form_for @article, html: { multipart: true }   do |article|%>
<%= article.label :title%>
<%= article.text_field :title%><br><br>
<%= article.label :avatar%>
<%#= article.file_field :avatars, multiple: true%>
